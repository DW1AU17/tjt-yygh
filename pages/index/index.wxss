.container .form {
  width: 65%;
  margin-top: 15%;
  margin-bottom: 30rpx;
}
.number {
  border: 1rpx solid #999;
  border-radius: 10rpx;
  height: 70rpx;
  padding-left: 15rpx;
  margin-top: 15rpx;
}
.number input {
  height: 100%;
}
button {
  width: 65%;
  margin-bottom: 20rpx;
}
image {
  width: 400rpx;
  height: 350rpx;
}
.id-card {
  margin-top: 20rpx;
}
.fs26 {
  font-size: 26rpx;
}
/* 预约必读 */
.order-required {
  width: 100%;
  height: 100%;
  position: fixed;
  background-color: #fff;
  z-index: 99;
  color: #444;
  font-size: 30rpx;
}
.order-required .title {
  text-align: center;
  color: #333;
  font-size: 38rpx;
}
.order-required view {
  margin-bottom: 20rpx;
  line-height: 50rpx;
}
.red {
  color: red;
}
.order-required .or-top {
  height: 90%;
  padding: 30rpx;
  box-sizing: border-box;
  overflow-y: scroll;
}
.order-required .or-bottom {
  height: 10%;
  padding: 0 20rpx;
  background-color: #eee;
  box-shadow: 2rpx 2rpx 12rpx 3rpx #666;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.or-bottom button {
  height: 60rpx;
  line-height: 60rpx;
  width: 200rpx;
  font-size: 32rpx;
  background-color: orange;
  color: #fff;
}
.hide {
  display: none;
}

